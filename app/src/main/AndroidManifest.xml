<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.isa.sahabatbunda">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/title_activity_ibuhamil"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IbuHamil" />
        <activity
            android:name=".DataKehamilan"
            android:label="@string/title_activity_data_kehamilan"
            android:parentActivityName=".IbuHamil"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.isa.sahabatbunda.IbuHamil" />
        </activity>
        <activity
            android:name=".Amnesa"
            android:label="@string/title_activity_amnesa"
            android:parentActivityName=".DataKehamilan"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.isa.sahabatbunda.DataKehamilan" />
        </activity>
    </application>

</manifest>
